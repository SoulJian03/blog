&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">sessions -i &amp;lt;session_id&amp;gt;
&lt;/span>&lt;/span>&lt;/code>&lt;/pre>&lt;/td>&lt;/tr>&lt;/table>
&lt;/div>
